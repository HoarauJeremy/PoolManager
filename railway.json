{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "dockerfile"
    },
  "deploy": {
    "preDeployCommands": [
        "npm i",
        "npm run build",
        "composer i", 
        "symfony console d:migrations:migrate"
    ],
    "startCommand": "echo starting!",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "never"
    }
}